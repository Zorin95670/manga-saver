{
  "manifest_version": 3,
  "name": "Manga saver",
  "version": "1.1.1",
  "description": "Chrome extension to save your progress when you read manga/webtoon online!",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
